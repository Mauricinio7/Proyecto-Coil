package coilvic.modelo.pojo;


public class profesorUv {
    String correo;
    String nombre;
    String noPersonal;
    String region;
    Integer idProfesorUv;

    public profesorUv() {
    }

    public profesorUv(String correo, String nombre, String noPersonal, String region, Integer idProfesorUv) {
        this.correo = correo;
        this.nombre = nombre;
        this.noPersonal = noPersonal;
        this.region = region;
        this.idProfesorUv = idProfesorUv;
    }

    public String getCorreo() {
        return correo;
    }

    public String getNombre() {
        return nombre;
    }

    public String getNoPersonal() {
        return noPersonal;
    }

    public String getRegion() {
        return region;
    }

    public Integer getIdProfesorUv() {
        return idProfesorUv;
    }

    public void setCorreo(String correo) {
        this.correo = correo;
    }

    public void setNombre(String nombre) {
        this.nombre = nombre;
    }

    public void setNoPersonal(String noPersonal) {
        this.noPersonal = noPersonal;
    }

    public void setRegion(String region) {
        this.region = region;
    }

    public void setIdProfesorUv(Integer idProfesorUv) {
        this.idProfesorUv = idProfesorUv;
    }
    
}
